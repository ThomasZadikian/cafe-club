  "dependencies": {
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.2.10",
    "i18next": "^23.15.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "multer": "^1.4.5-lts.1",
    "postcss": "^8.4.45",
    "promise": "^8.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.11",
    "util": "^0.12.5",
    "web-vitals": "^4.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.6.2"
  },
  "scripts": {
    "start": "concurrently \"npm run tailwind:watch\" \"npm run start:client\"",
    "start:client": "react-scripts start",
    "build": "npm run tailwind:build && react-scripts build",
    "tailwind:build": "NODE_ENV=production npx tailwindcss build src/index.css -o src/tailwind.css",
    "tailwind:watch": "npx tailwindcss -i ./src/input.css -o ./src/dist/output.css --watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
