{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^11.2.10",
    "i18next": "^23.11.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.0",
    "promise": "^8.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.3",
    "util": "^0.12.5",
    "web-vitals": "^4.0.1"
  },
  "scripts": {
    "start": "concurrently \"npm run tailwind:watch\" \"npm run start:client\" \"npm run start:server\"",
    "start:client": "react-scripts start",
    "start:server": "node ./src/back/express.js",
    "build": "npm run tailwind:build && react-scripts build",
    "tailwind:build": "NODE_ENV=production npx tailwindcss build src/index.css -o src/tailwind.css",
    "tailwind:watch": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
